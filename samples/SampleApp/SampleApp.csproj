<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <AspNetCoreHostingModel>inprocess</AspNetCoreHostingModel>
    <NoWarn>$(NoWarn);CA1062;CA2007;SA1600</NoWarn>
    <TargetFramework>net6.0</TargetFramework>
